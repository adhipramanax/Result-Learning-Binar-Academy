{
  "name": "back-end",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config bin/www",
    "eslint": "eslint app",
    "develop": "nodemon -r dotenv/config bin/www",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "test": "jest --coverage",

    "db:create_test": "sequelize db:create --env test",
    "db:drop_test": "sequelize db:drop --env test",
    "db:migrate_test": "sequelize db:migrate --env test",
    "db:seed_test": "sequelize db:seed_test:undo:all && sequelize db:seed:all --env test"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "sequelize": "^6.20.1",
    "sequelize-cli": "^6.4.1",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3"
  },
  "description": "Di dalam repository ini terdapat implementasi API dari Binar Car Rental.\r Tugas kalian disini adalah:\r 1. Fork repository\r 2. Tulis unit test di dalam repository ini menggunakan `jest`.\r 3. Coverage minimal 70%",
  "directories": {
    "doc": "docs"
  },
